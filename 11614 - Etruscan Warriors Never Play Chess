#include <bits/stdc++.h> 
using namespace std; 
int main() 
{ 
    int t;
    cin>>t;
    while(t--)
    {
        long long k,n;
        cin>>k;
        float sol;
        sol=(float)(sqrt(8*k+1)-1)/2;
        n=(long long)sol;
         if(sol>n)
        cout<<n+1<<'\n';
          else
         cout<<n<<'\n';
    }
	return 0; 
} 

#include <bits/stdc++.h> 
using namespace std; 
int main() 
{ 
    int n,r;
    while(cin>>r>>n)
    { if(r==0 || n==0)
    exit(0);
        int sol,test=1;
       // float res;
        sol = (int)r/n;
       // sol = ceil(res);
         if(sol>27)
         cout<<"Case "<<test<<": "<<"impossible"<<'\n';
          else
         cout<<"Case "<<test<<": "<<sol<<'\n';
         test++;
    }
	return 0; 
} 
